{
  "name": "@nebula/wasm",
  "version": "0.1.0",
  "description": "WebAssembly support for NebulaDB",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:wasm": "cd wasm && wasm-pack build --target web",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "database",
    "nosql",
    "wasm",
    "webassembly",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@nebula/core": "^0.1.0"
  }
}
