{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "error"
  },
  "overrides": [
    {
      "files": [
        "**/benchmarks/**/*.ts",
        "**/benchmarks/**/*.js",
        "**/tests/**/*.ts",
        "**/tests/**/*.js",
        "**/examples/**/*.ts",
        "**/examples/**/*.js",
        "**/cli/**/*.ts",
        "**/cli/**/*.js",
        "**/devtools/**/*.ts",
        "**/devtools/**/*.js",
        "**/vscode-extension/**/*.ts",
        "**/vscode-extension/**/*.js",
        "**/plugins/**/*.ts",
        "**/plugins/**/*.js",
        "**/plugin-sync/**/*.ts",
        "**/plugin-sync/**/*.js"
      ],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
